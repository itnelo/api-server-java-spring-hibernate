<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="
     http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
     http://www.springframework.org/schema/util  http://www.springframework.org/schema/util/spring-util-3.1.xsd">

    <beans profile="default,!default">
        <util:map id="apiRequestStructure">
            <entry key="CLIENT_IDENTIFIER" value="client_key"/>
            <entry key="ACCESS_KEY" value="api_access_key"/>
            <entry key="API_VERSION" value="api_version"/>
            <entry key="API_HANDLER" value="api_query_key"/>
            <entry key="QUERY_DATA" value="api_query_data"/>
        </util:map>
    </beans>

</beans>